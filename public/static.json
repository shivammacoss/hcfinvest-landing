{
  "cleanUrls": true,
  "trailingSlash": false,
"headers": {
    "/**/*.png": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.jpg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.jpeg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.webp": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.svg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.gif": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/index.html": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    }
  },
  "redirects": [
    { "source": "/**", "destination": "/", "type": 404 }
  ]
}